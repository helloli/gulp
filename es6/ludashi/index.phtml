    play: function() {
      var self = this;

      jumbotronInited = true;
      this.jumbotronShowed = true;

      // if (this.$replay.css('visibility') == 'hidden') {
      //   se_monitor.customizeLog({
      //     event_name: 'track',
      //     para: {
      //       act: 'track',
      //       pid: 'p_jumbotron_replay'
      //     }
      //   });
      //   se_monitor.customizeLog({
      //     event_name: 'track',
      //     para: {
      //       act: 'track',
      //       pid: (self.pid ? self.pid : 'p_jumbotron_small')
      //     }
      //   });
      // }

      this.$replay.css({
        'visibility': 'visible'
      });
      this.$container.css({
        width: '100%',
        height: '290px',
        bottom: '0',
        left: '0',
        opacity: 1
      }).slideDown(function() {
        self._record();
      });

      // se_monitor.customizeLog({
      //   event_name: 'track',
      //   para: {
      //     act: 'track',
      //     pid: 'p_jumbotron'
      //   }
      // });

      // se_monitor.customizeLog({
      //   event_name: 'track',
      //   para: {
      //     act: 'track',
      //     pid: 'p_jumbotron_skip'
      //   }
      // });
      var counter = 0;

      clearInterval(this.playTimer);
      this._renderTimeout(this.PLAY_TIMEOUT - counter);
      this.playTimer = setInterval(function() {
        if (++counter <= self.PLAY_TIMEOUT) {
          self._renderTimeout(self.PLAY_TIMEOUT - counter);
        } else {
          self.stop();
        }
      }, 1e3);
    },

    stop: function() {
      clearTimeout(this.delayTimer);
      clearInterval(this.playTimer);

      var self = this;
      if (this._offset) { // 增加修改收起位置的特性 liuguanyu 2017-09-11
        this.$container.animate({
          width: '0',
          height: '0',
          bottom: this._offset.bottom + 'px',
          left: this._offset.left + 'px',
          opacity: 0
        }, function() {
          jumbotronInited = false;
          self.$container.hide();
        });
      } else {
        this.$container.animate({
          width: '0',
          height: '0',
          bottom: '300px',
          left: '680px',
          opacity: 0
        }, function() {
          jumbotronInited = false;
          self.$container.hide();
        });
      }
    },

    init: function(ad) {
      jumbotronInited = false;
      if (!ad.lcurl || !ad.limg || !this.needShow()) return;
      jumbotronInited = true;

      this.$container.find('.btn-link').attr({
        href: ad.lcurl,
        ctype: 1,
        clktks: ad.clktk.join('|')
      });
      this.$container.find('img').attr({
        src: ad.limg
      });
      this.$replay.data('acttk', ad.acttk);

      var self = this;
      var consumedTime = new Date().getTime() - startTime;

      this.delayTimer = setTimeout(function() {
        var $cur = $('#nav a.cur');
        var bk = $cur.attr('bk');
        if (bk != 'nav_rec') {
          jumbotronInited = false;
        } else {
          self.play();
        }
      }, this.DELAY_TIMEOUT - consumedTime);
    }
  };
