      //   });
      // } else { //有开屏
      //   if (showids !== jumbotron.getShowIds()) { // 开屏不是当前图，需要打track，是当前图的情况，jumbotron已经打了
      //     se_monitor.customizeLog({
      //       event_name: 'track',
      //       para: {
      //         act: 'track',
      //         pid: data.ext.no // 打no的pid
      //       }
      //     });
      //   }
      // }
    }

    //  开屏逻辑
    var doKaiping = function(data) {
      var impurl = data && data.impurl;
      var showids = data.showids;
      var ads = data && data.adspaces && data.adspaces[showids];
      ads = ads && ads.ads && ads.ads[0];

      if (!ads) return; // 没有广告数据，返回

      var $container = data.$container;
      jumbotron.init(ads);
      jumbotron.setShowIds(showids);
      jumbotron.setPid(data.ext.has);
      jumbotron.setReplay($container.find(".btn-replay"));
      jumbotron.setFadeOffset(data.getCenterOffset());
    }

    // 请求mediaV接口
    var requestMediaV = function(showids) {
      return $.ajax({
        url: '//show-3.mediav.com/s?type=1&of=4&newf=1',
        // url: '//show-g.mediav.com/s?type=1&of=4&newf=2',
        data: {
          // showids: showids,
          // mid: mid
          showid: showids,
          uid: mid
        },
        dataType: "jsonp",
        jsonp: "jsonp",
        jsonpCallback: "callback_mediav_" + showids,
        timeout: 3000
      });
    };


    // 有master，用master显示，否则选择slave的第一个
    var manageAd = function(datas) {
      var master = $.grep(datas, function(val) {
        return val.isMaster == true && isValidKaiping(val);
      });

      if (master.length) {
        master = master[0];
      } else {
        master = $.grep(datas, function(val) {
          return isValidKaiping(val);
        });

        master = master[0] ? master[0] : "";
      }

      if (master !== "") { // 有需要开屏的，进行开屏展示
        doKaiping(master);
      }

      $.each(datas, function(i, el) { // 无论如何都要替换广告
        // 替换广告
        doAd(el);

        setTimeout(function() {
          el.showHideContainer(true);
        }, 50);
      });
    };

    function MediaNode($container, showids, ext) {
      this.$container = $container;
      this.showids = showids;
      this.ext = ext;
      this.isMaster = false;
    }

    MediaNode.prototype = {
      // 显示或隐藏container
      showHideContainer: function(bool) {
        this.$container.css({
          visibility: !!bool ? "visible" : "hidden"
        });
      },

      // 获取中心的位置
      getCenterOffset: function() {
        var offset = this.$container.offset();

        var left = offset.left + this.$container.width() / 2;

        var spagesHeight = this.$container.parents(".wrapper").height();

        var bottom = spagesHeight - (this.$container.height() / 2) - offset.top;

        return { left: left, bottom: bottom };
      }
    };

    // 维护开屏展示的对象
    var showMediavMasterSlave = {
      slave: [],

      setMaster: function(master) {
        master.isMaster = true;
        this.master = master;

        return this;
      },

      addSlave: function(slave) {
        this.slave.push(slave);
        return this;
      },

      run: function() {
        var self = this;
        var nodes = this.master ? $.merge([this.master], this.slave) : this.slave;

        var reqs = $.map(nodes, function(el) {
          el.showHideContainer(false);

          return Q.resolve(requestMediaV(el.showids))
            .then(function(data) {
              var elObj = $.extend(true, {}, data, {
                "isMaster": el.isMaster,
                "$container": el.$container,
                "showHideContainer": el.showHideContainer,
                "showids": el.showids,
                "ext": el.ext,
                "getCenterOffset": el.getCenterOffset
              });

              return elObj;
            }, function() { // 失败时显示回来
              el.showHideContainer(true);
            });
        });

        Q.Promise.all(reqs)
          .then(function(rets) {
            // 取不回数据的直接回显
            var noData = $.grep(rets, function(el) {
              return el === undefined;
            });

            $.each(function(i, el) {
              el.showHideContainer(true);
            });

            // 其他的数据进入广告开屏逻辑
            var datas = $.grep(rets, function(el) {
              return el !== undefined;
            });

            manageAd.call(self, datas);
          });
      }
    };

    // var slave = new MediaNode($('[bk=recommend_right_ads_big]'), 'qy8I4Y');
    // var master = new MediaNode($('[bk=recommend_right_ads_small]'), 'a0ZVnP');

    var slave = new MediaNode($('[bk=recommend_right_ads_big]'), 'eFACGC', {
      "has": "p_jumbotron_small",
      "no": "p_recommend_right_ads"
    }); // 大图，副
    var master = new MediaNode($('[bk=recommend_right_ads_small]'), 'Rj3HQM', {
      "has": "p_jumbotron_rightbottom_small",
      "no": "p_rightbottom_smll"
    }); // 小图，主

    showMediavMasterSlave
      .setMaster(master)
      .addSlave(slave)
      .run();

    /*************************
     * liuguanyu 2017-09-10 以上
     *************************/

    // showcase start
    window.showcaseValid = false;
    window.showcaseVisible = false;
    var donttingMap = {};

    function renderShowcase($showcase, ads, impurl) {
      var $first = $showcase.find('.item-first');
      var $list = $showcase.find('.item-list ul');
      var $bottom = $showcase.find('.item-second');
      var $placeholder = $('.showcase-placeholder');
      var topGroupIndex = 0;
      var bottomGroupIndex = 0;
      var bottomSlideGroup = ads.splice(0, 1);
      var topSlideGroup = ads.slice(0, 4);
      var slideTimer = null;
      var isMouseEnter = false;
      var tpl = '<li class="thumb"><img src="" alt="" width="34" height="34"></li>';

      $('body').on('showcaseVisible' + $showcase.selector.replace('.', '-'), function(e) {
        dotting([topSlideGroup[0], bottomSlideGroup[0]]);
      });

      function renderTop() {
        render($first, topSlideGroup[topGroupIndex]);
        $list.find('li img').removeClass('active');
        $list.find('li').eq(topGroupIndex).find('img').addClass('active');
      }

      function renderBottom() {
        render($bottom, bottomSlideGroup[0]);
      }

      renderTop();
      renderBottom();
      dotting([topSlideGroup[0], bottomSlideGroup[0]]);

      function dotting(list) {
        if (list.length == 1 && topGroupIndex == 0) return;

        var keys = [];
        for (var i = 0; i < list.length; i++) {
          keys.push(list[i].curl);
        }
        var key = keys.join('_');

        if (!donttingMap[key] && showcaseVisible) {
          donttingMap[key] = true;

          var args = [];
          list.forEach(function(a) {
            (a.imptk || []).forEach(function(item) {
              item && (new Image().src = item);
            });

            args.push(a.imparg);
          });
          new Image().src = [impurl, args.join('.')].join('');
        }
      }

      function render($item, ad) {
        $item.find('a').attr({
          href: ad.curl,
          ctype: '1',
          'data-clktk': (ad.clktk || []).join(',')
        });

        var needClip = (ad.imgw == 280) && (ad.imgh == 200);
        var src = needClip ? ('http://imgx.mediav.com/rimg.png?size=180x180&img=' + ad.img + '&resizetype=3&quality=100&axisreset=1') : ad.img;

        $item.find('img').attr({
          src: src,
          alt: ad.desc
        });
        $item.find('.item-title').text(ad.desc);
      }

      function renderList() {
        var html = '';
        for (var i = 0; i < topSlideGroup.length; i++) {
          var ad = topSlideGroup[i];
          var $li = $(tpl);
          $li.find('img').attr('src', ad.img);
          html += $li[0].outerHTML;
        }
        $list.html(html);
      }

      function prePage() {
        topGroupIndex -= 1;
        bottomGroupIndex -= 1;

        if (topGroupIndex < 0) {
          topGroupIndex = topSlideGroup.length - 1;
        }

        renderTop();
        dotting([topSlideGroup[topGroupIndex]]);
      }

      function nextPage() {
        topGroupIndex += 1;
        bottomGroupIndex += 1;

        if (topGroupIndex > topSlideGroup.length - 1) {
          topGroupIndex = 0;
        }

        renderTop();
        dotting([topSlideGroup[topGroupIndex]]);
      }

      function startTimer() {
        var timeout = topGroupIndex == 0 ? 7e3 : 5e3;

        clearTimeout(slideTimer);
        slideTimer = setTimeout(function() {
          if (!isMouseEnter) {
            nextPage();
          }
          startTimer();
        }, timeout);
      }

      $placeholder.css('visibility', 'hidden');
      renderList();
      renderTop();
      renderBottom();

      $showcase.on('mouseenter', '.arrow-left', function(e) {
        e.preventDefault();
        e.stopPropagation();
        prePage();
        startTimer();
      }).on('click', '.arrow-left', function(e) {
        e.preventDefault();
        e.stopPropagation();
        prePage();
        startTimer();
      }).on('mouseenter', '.arrow-right', function(e) {
        e.preventDefault();
        e.stopPropagation();
        nextPage();
        startTimer();
      }).on('click', '.arrow-right', function(e) {
        e.preventDefault();
        e.stopPropagation();
        nextPage();
        startTimer();
      }).on('click', 'a', function(e) {
        var clktk = $(this).attr('data-clktk').split(',');
        clktk.forEach(function(item) {
          item && (new Image().src = item);
        });
      }).on('click', 'ul li', function(e) {
        e.preventDefault();
        topGroupIndex = $(this).index();
        renderTop();
      }).on('mouseenter', 'ul li', function(e) {
        e.preventDefault();
        topGroupIndex = $(this).index();
        renderTop();
      }).on('mouseenter', '.top img', function(e) {
        isMouseEnter = true;
      }).on('mouseleave', '.top img', function(e) {
        isMouseEnter = false;
      });

      setTimeout(function() {
        startTimer();
      }, 10000);
    }

    function fetchShowcase(showid, $container) {
      // @sea http://pages.juxiao.mediav.com/mdoc/pc_api/HTML/SceneWindowDockingDoc.html
      $.ajax({
        url: '//show-3.mediav.com/s',
        data: {
          type: 1,
          newf: 1,
          of: 4,
          showid: showid, //'niE1LX', //mTZATM',
          mid: mid,
          uid: mv_uid,
          impct: 5
        },
        dataType: 'jsonp',
        jsonp: 'jsonp',
        timeout: 5000,
        success: function(data) {
          var ads = (data || {}).ads || [];

          if (ads.length >= 5) {
            showcaseValid = true;
            renderShowcase($container, ads, data.impurl);
          }
        }
      });
    }

    fetchShowcase('mTZATM', $('.showcase-1'));
    fetchShowcase('niE1LX', $('.showcase-2'));
    // showcase end

    // init PageSlider
    $('.page .page-scrollbar').each(function() {
      new PageSlider($(this));
    });
  });
